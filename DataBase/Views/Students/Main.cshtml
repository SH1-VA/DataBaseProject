@model IList<DataBase.Storage.Entity.Student>

	<h1>Студенты:</h1>

	<table>
		<tr>
			<th>
				ID
			</th>
			<th>
				Фамилия
			</th>
			<th>
				Имя
			</th>
			<th>
				Номер телефона
			</th>
		</tr>
		@foreach(var student in Model)
		{
			<tr>
				<td>@student.Id</td>
				<td>@student.LastName</td>
				<td>@student.Name</td>		
				<td>@student.PhoneNumber</td>
				<td>@student.Email</td>
			</tr>
		}
	</table>